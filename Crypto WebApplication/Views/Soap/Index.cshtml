
@{
    ViewData["Title"] = "Soap search";

}


<form asp-action="Search" method="post">

    <div class="mb-3">
        <label for="keyword">Circulating supply greater than:</label>
        <input type="number" class="form-control" name="keyword" placeholder="32" required>
        <div class="invalid-feedback">
            Please enter circulating supply.
        </div>
    </div>

    <hr class="mb-4">
    <button class="btn btn-primary btn-lg btn-block" type="submit">Search</button>
</form>
